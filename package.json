{
  "name": "enterprise-management-system",
  "version": "1.0.0",
  "private": true,
  "description": "现代化企业级管理系统前端应用",
  "keywords": [
    "enterprise",
    "management",
    "system",
    "nextjs",
    "react",
    "typescript",
    "tailwindcss",
    "ai",
    "dashboard"
  ],
  "homepage": "https://github.com/your-org/enterprise-management-system#readme",
  "bugs": {
    "url": "https://github.com/your-org/enterprise-management-system/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/enterprise-management-system.git"
  },
  "license": "MIT",
  "author": {
    "name": "开发团队",
    "email": "dev@company.com",
    "url": "https://company.com"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "build": "next build",
    "clean": "rimraf .next out dist",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "dev": "next dev",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare": "husky install",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.39",
    "@ai-sdk/google": "^0.0.37",
    "@ai-sdk/openai": "^0.0.42",
    "@hookform/resolvers": "^3.3.4",
    "@next/bundle-analyzer": "^14.1.0",
    "@prisma/client": "^5.10.2",
    "@radix-ui/react-accordion": "latest",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "latest",
    "@radix-ui/react-checkbox": "latest",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "latest",
    "@radix-ui/react-label": "latest",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "latest",
    "@radix-ui/react-scroll-area": "latest",
    "@radix-ui/react-select": "latest",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "latest",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "latest",
    "@radix-ui/react-tabs": "latest",
    "@radix-ui/react-toast": "latest",
    "@radix-ui/react-tooltip": "latest",
    "@tanstack/react-query": "^5.28.4",
    "@tanstack/react-table": "^8.15.3",
    "ai": "^3.0.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.0.24",
    "lucide-react": "^0.454.0",
    "next": "14.1.4",
    "next-auth": "^4.24.7",
    "next-themes": "latest",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.1",
    "react-hot-toast": "^2.4.1",
    "recharts": "latest",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
